  "description": "web-worker_partytown",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrey-Drozd/web-worker_partytown"
  },
  "scripts": {
    "start": "cross-env webpack serve --config config/webpack/webpack.config.js",
    "dev": "cross-env webpack --config config/webpack/webpack.config.js",
    "build": "npm run all-cheks && cross-env NODE_ENV=production webpack --config config/webpack/webpack.config.js",
    "bundle:analyzer": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack --config config/webpack/webpack.config.js",
    "all-cheks": "npm run lint && npm run lint:styles && npm run ts:check",
    "ts:check": "tsc",
    "lint": "eslint --config .eslintrc.json .",
    "lint:fix": "eslint --config .eslintrc.json . --fix",
    "lint:styles": "stylelint src/**/*.scss",
    "lint:styles:fix": "stylelint src/**/*.scss --fix"
  },
  "dependencies": {
    "core-js": "^3.22.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/plugin-syntax-jsx": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.17.12",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.13",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.5.0",
    "prettier": "2.6.2",
    "react-refresh": "^0.13.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
